{
	"name": "Node.js over Alpine",
	"build": {
		"dockerfile": "Dockerfile",
		"context": ".."
	},
	"features": {
		"ghcr.io/devcontainers/features/common-utils:2": {
			"installZsh": "true",
			"username": "vscode",
			"upgradePackages": "true"
		}
	},
	"remoteUser": "vscode",
	"customizations": {
		"vscode": {
			"extensions": []
		}
	},
	"forwardPorts": [3000]
}
